SCRIPTPATH=$( cd "$(dirname "$0")" ; pwd -P )
APP_VERSION=$(python -c "from ggrc.settings.default import VERSION; print(VERSION)")

cd "${SCRIPTPATH}/.."

# add VERSION to env variables list
export APP_VERSION

npm run build-release-notes
