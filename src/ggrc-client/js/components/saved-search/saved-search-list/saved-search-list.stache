<h3>
  Saved Searches
</h3>

<ul>
  {{#each searches}}
    <li ($click)="applySearch(.)"
     {{#if disabled}}class="disabled"{{/if}}>
      <span>{{name}}</span>
      <i class="fa fa-trash"
        ($click)="removeSearch(., %event)"
      ></i>
    </li>
  {{/each}}
</ul>
