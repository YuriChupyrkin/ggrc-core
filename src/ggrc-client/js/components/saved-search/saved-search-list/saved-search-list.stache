<h3>
  Saved Searches
</h3>

<ul>
  {{#each searches}}
    <li on:el:click="applySearch(.)"
     {{#if disabled}}class="disabled"{{/if}}>
      <span title="{{name}}">{{name}}</span>
      <i class="fa fa-trash"
        on:el:click="removeSearch(., scope.event)"
      ></i>
    </li>
  {{/each}}
</ul>
