<h3>
  Saved Searches
</h3>

<ul>
  {{#each searches}}
    <li>
      <span>{{name}}</span>
      <i ($click)="removeSearch(.)" class="fa fa-trash"></i>
    </li>
  {{/each}}
</ul>
