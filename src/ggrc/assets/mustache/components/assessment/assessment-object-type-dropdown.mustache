<select
  class="{{cssClass}}"
  can-value="assessmentType">
  {{#each objectTypes}}
    {{#if items}}
      {{#if items.length}}
      <optgroup label="{{name}}">
        {{#each items}}
          {{#if name}}
            <option value="{{value}}" label="{{name}}"></option>
          {{/if}}
        {{/each}}
      </optgroup>
      {{/if}}
    {{/if}}
  {{/each}}
</select>
